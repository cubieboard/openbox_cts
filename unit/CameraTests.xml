                                                <Test name="testSwitchMode"/>                        
                                        </TestCase>                    
                                        <TestCase name="CameraStartUp">                        
                                                <Test name="testLaunchVideo"/>                        
                                                <Test name="testLaunchCamera"/>                        
                                        </TestCase>                    
