<?xml version="1.0" encoding="UTF-8"?>
<TestPackage AndroidFramework="Android 1.0"
	 appNameSpace="com.android.camera.tests" 
	 appPackageName="android.camera" name="CameraTests"
	 runner="android.test.InstrumentationTestRunner" 
	 targetNameSpace="com.android.camera" version="1.0">    
        <TestSuite name="com">        
                <TestSuite name="android">            
                        <TestSuite name="camera">    
                                <TestSuite name="functional">                    
                                      <TestCase name="ImageCaptureIntentTest">                        
                                           <Test name="testNoExtraOutput"/>                        
                                           <Test name="testRetake"/>  
                                           <Test name="testCancel"/>   
                                           <Test name="testSnapshotCancel"/>                         
                                       </TestCase> 
                                        <TestCase name="VideoCaptureIntentTest">                        
                                           <Test name="testNoExtraOutput"/>                        
                                           <Test name="testRetake"/>  
                                           <Test name="testCancel"/>   
                                           <Test name="testSnapshotCancel"/>  
                                           <Test name="testExtraSizeLimit"/>  
